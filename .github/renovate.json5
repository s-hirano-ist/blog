{
  baseBranches: ["main"],
  extends: ["config:base", ":semanticCommitTypeAll(chore)"],
  timezone: "Asia/Tokyo",
  enabledManagers: ["npm"],
  lockFileMaintenance: {
    enabled: true,
    autoMerge: true,
  },
  dependencyDashboard: false,
  schedule: ["before 9pm on Monday"],
  automergeSchedule: ["before 9pm on Monday"],
  labels: ["dependencies"],
  rangeStrategy: "bump",
  packageRules: [
    {
      matchManagers: ["npm"],
      matchUpdateTypes: ["patch"],
      matchPackagePatterns: ["*"],
      matchDepTypes: ["dependencies", "devDependencies", "peerDependencies"],
      groupName: "patch",
      autoMerge: true,
    },
    {
      matchManagers: ["npm"],
      matchUpdateTypes: ["minor"],
      matchPackagePatterns: ["*"],
      matchDepTypes: ["dependencies", "devDependencies", "peerDependencies"],
      groupName: "minor",
      autoMerge: true,
    },
  ],
}

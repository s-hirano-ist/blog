{
  timezone: "Asia/Tokyo",
  baseBranches: ["main"],
  extends: ["config:base", ":semanticCommitTypeAll(chore)"],
  enabledManagers: ["npm"],
  dependencyDashboard: true,
  schedule: ["before 11am every weekday"],
  automergeSchedule: ["before 11am every weekday"],
  labels: ["dependencies"],
  rangeStrategy: "bump",
  lockFileMaintenance: {
    enabled: true,
    automerge: true,
  },
  packageRules: [
    {
      matchManagers: ["npm"],
      matchDepTypes: ["dependencies", "devDependencies", "peerDependencies"],
      matchPackagePatterns: ["*"],
      automerge: true,
    },
  ],
}
